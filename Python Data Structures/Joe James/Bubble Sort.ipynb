    "def bubble_sort(A):\n",
    "    for i in range(0 , len(A) - 1):\n",
    "        for j in range(0 , len(A) - 1 - i):\n",
    "            if A[j] > A[j+1]:\n",
    "                A[j] , A[j+1] = A[j+1] , A[j]\n",
    "                \n",
    "    return A"
   ]
